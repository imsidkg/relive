{
  "buildCommand": "cd ../backend && bun install && bunx prisma generate && cd ../frontend && bun run build",
  "devCommand": "bun run dev",
  "installCommand": "bun install",
  "framework": "vite",
  "outputDirectory": "dist",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
